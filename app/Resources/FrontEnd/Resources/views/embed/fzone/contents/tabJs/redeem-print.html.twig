                        <div style="font-family:'FuturaBT-Book', Arial, Helvetica, sans-serif; color: #006bb5 !important; font-size:28px; font-weight:bold; text-transform:uppercase;">{{ 'fzone.myvouchers.instruction.head1_redeemable'|trans({}, 'frontend')|raw }}</div>
                        <div style="font-family:'FuturaBT-Book', Arial, Helvetica, sans-serif; font-size:20px; color:#000000;">{{ 'fzone.myvouchers.instruction.txt1_redeemable'|trans({}, 'frontend')|raw }}</div>
                    {% endif %}
