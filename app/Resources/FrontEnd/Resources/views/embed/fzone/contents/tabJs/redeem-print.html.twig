{%  set fullUrl = s3Url ~ '/images/print/' ~ priceType %}
<div class="print-this-reward">
    <div class="enPortal-row" style="text-align: center;">
        <img src="{{ fullUrl }}logo-site-newer-GoPlay.png" style="margin-bottom:20px;" />
        <img src="{{ fullUrl }}red-bar.png"/>
        <h1 style="margin: 10px 0px; color: #000 !important; font-family:Arial; font-size: 36px;">{{ voucher.title }}</h1>
        <img src="{{ fullUrl }}top-red-bar.png"/>
    </div>
    <table width="100%" style="margin-bottom: 10px;">
        <tr>
            <td width="2%"></td>
            <td width="85%" style="font-family:'FuturaBT-Book', Arial, Helvetica, sans-serif; vertical-align: top; padding-top: 10px; font-size: 15px !important; color: #000 !important;">
                <p style="text-align: center; font-size:20px;">{{ voucher.description|raw }}</p>
            </td>
            <td width="2%"></td>
        </tr>
    </table>
    <div class="enPortal-row" style="text-align: center;">
        <img src="{{ fullUrl }}red-bar.png"/>
    </div>
    <div class="enPortal-row" style="text-align: center;">
