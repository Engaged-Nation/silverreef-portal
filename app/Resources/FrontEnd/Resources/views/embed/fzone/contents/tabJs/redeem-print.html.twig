                    {{ 'fzone.myvouchers.header_redeemable'|trans({}, 'frontend')|raw }}
                {% endif %}
            </div>
        {% else %}
            <div style='font-family:"FuturaBT-Book", Arial, Helvetica, sans-serif; color: #006bb5 !important;
